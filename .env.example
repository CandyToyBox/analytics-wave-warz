# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_KEY=your-anon-key-here

# API Server Configuration
PORT=3001
NODE_ENV=development

# Farcaster Frame Configuration
FRAME_BASE_URL=https://your-api-domain.com
APP_URL=https://your-app-domain.com

# Webhook Security (Optional)
WEBHOOK_SECRET=your-webhook-secret-here

# Admin API Secret (for /api/admin/* endpoints)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ADMIN_SECRET=your-secure-random-string-here

# Helius RPC URL (for Solana blockchain data)
VITE_HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your-helius-api-key
